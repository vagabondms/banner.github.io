{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@hooks": ["src/hooks"],
      "@hooks/*": ["src/hooks/*"],
      "@utils": ["src/utils"],
      "@utils/*": ["src/utils/*"],
      "@containers/*": ["src/containers/*"],
      "@atoms/*": ["src/components/atoms/*"],
      "@molecules/*": ["src/components/molecules/*"],
      "@organisms/*": ["src/components/organisms/*"],
      "@stories/*": ["src/stories/*"],
      "@api": ["src/api"],
      "@/*": ["src/*"]
    }
  }
}
